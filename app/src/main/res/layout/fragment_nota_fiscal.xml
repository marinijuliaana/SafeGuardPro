<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.LoginFragment">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/materialToolbar"
        android:layout_width="462dp"
        android:layout_height="74dp"
        android:background="#C47D12"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="-43dp"
        tools:layout_editor_absoluteY="0dp" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="692dp"
        android:layout_height="698dp"
        android:src="@drawable/epi2_removebg_preview"
        tools:layout_editor_absoluteX="-141dp"
        tools:layout_editor_absoluteY="59dp" />

    <View
        android:id="@+id/view"
        android:layout_width="299dp"
        android:layout_height="340dp"
        android:background="#DDD5D5"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="212dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="119dp"
        android:layout_height="51dp"
        android:backgroundTint="#008000"
        android:text="EMITIR"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="475dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOTA FISCAL"
        android:textSize="25sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="250dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="75dp"
        android:layout_height="69dp"
        android:src="@drawable/epiorg"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="5dp" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Data Emissão"
        android:inputType="date"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="319dp" />

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Número da nota fiscal"
        android:inputType="number"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="398dp" />

</androidx.constraintlayout.widget.ConstraintLayout>